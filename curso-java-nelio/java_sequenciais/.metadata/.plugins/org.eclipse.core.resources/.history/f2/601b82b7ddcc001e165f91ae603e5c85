package casting;

public class Main {

	public static void main(String[] args) {
		double a, b;
		double result;
		
		a = 5;
		b = 2;
		result = a / b;
		
		// Without casting
		System.out.println("Resultado = " + result);
		
		// With casting
		result = (double) a / b;
		System.out.println("Resultado = " + result);
	}

}
